            android:theme="@style/Theme.MyTheme" />
        <activity
            android:name=".facilities.activity.RoomDetailActivity"
            android:label="@string/title_activity_room_detail"
            android:parentActivityName=".MITMainActivity"
            android:theme="@style/Theme.MyTheme" />
        <activity
            android:name=".facilities.activity.LocationActivity"
            android:label="@string/location_title"
            android:parentActivityName=".MITMainActivity"
            android:theme="@style/Theme.MyTheme" />
        <activity
            android:name=".facilities.activity.ProblemTypesActivity"
            android:label="@string/title_activity_problem_types"
