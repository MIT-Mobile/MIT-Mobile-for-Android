<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="edu.mit.mitmobile2">

    <permission
        android:name="com.vogella.android.locationapi.maps.permission.MAPS_RECEIVE"
        android:protectionLevel="signature" />

    <uses-feature
        android:glEsVersion="0x00020000"
        android:required="true" />

    <uses-permission android:name="com.vogella.android.locationapi.maps.permission.MAPS_RECEIVE" />
    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.ACCESS_COARSE_LOCATION" />
    <uses-permission android:name="android.permission.ACCESS_FINE_LOCATION" />
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />
    <uses-permission android:name="android.permission.CALL_PHONE" />
    <uses-permission android:name="android.permission.READ_PHONE_STATE" />
    <uses-permission android:name="android.permission.READ_CONTACTS" />
    <uses-permission android:name="android.permission.WRITE_CONTACTS" />
    <uses-permission android:name="android.permission.READ_SYNC_SETTINGS" />
    <uses-permission android:name="android.permission.WRITE_SYNC_SETTINGS" />
    <uses-permission android:name="android.permission.AUTHENTICATE_ACCOUNTS" />
    <uses-permission android:name="android.permission.READ_SYNC_STATS" />
    <uses-permission android:name="android.permission.VIBRATE" />
    <uses-permission android:name="android.permission.WAKE_LOCK" />
    <uses-permission android:name="android.permission.READ_CALENDAR" />
    <uses-permission android:name="android.permission.WRITE_CALENDAR" />

    <!-- android:theme="@android:style/Theme.Holo.Light" -->
    <application
        android:name=".MitMobileApplication"
        android:allowBackup="true"
        android:icon="@drawable/mit_product_icon"
        android:label="@string/app_name"
        android:theme="@style/MITMobile">
        <receiver android:name=".shuttles.AlarmReceiver" />

        <service android:name=".MITAuthenticatorService">
            <intent-filter>
                <action android:name="android.accounts.AccountAuthenticator" />
            </intent-filter>

            <meta-data
                android:name="android.accounts.AccountAuthenticator"
                android:resource="@xml/authenticator" />
        </service>
        <service
            android:name=".MITSyncService"
            android:exported="true"
            android:process=":sync">
            <intent-filter>
                <action android:name="android.content.SyncAdapter" />
            </intent-filter>

            <meta-data
                android:name="android.content.SyncAdapter"
                android:resource="@xml/syncadapter" />
        </service>

        <provider
            android:name=".shared.MITContentProvider"
            android:authorities="edu.mit.mitmobile2.provider"
            android:exported="false"
            android:label="MitProvider"
            android:syncable="true" />

        <activity
            android:name=".MITMainActivity"
            android:launchMode="singleTop"
            android:theme="@style/Theme.MyTheme">
            <intent-filter android:label="MIT Mobile">
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
            <intent-filter android:label="moduleSelector">
                <data android:scheme="mitmobile2" />

                <action android:name="android.intent.action.VIEW" />

                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>
        <activity
            android:name=".libraries.LibrariesActivity"
            android:label="@string/title_activity_libraries"
            android:launchMode="singleTop"
            android:theme="@style/Theme.MyTheme" />
        <activity
            android:name=".dining.activities.DiningActivity"
            android:label="@string/title_activity_dining"
            android:launchMode="singleTop"
            android:theme="@style/Theme.MyTheme" />
        <activity
            android:name=".qrreader.QrreaderActivity"
            android:label="@string/title_activity_qrreader"
            android:launchMode="singleTop"
            android:theme="@style/Theme.MyTheme" />
        <activity
            android:name=".links.LinksActivity"
            android:label="@string/title_activity_links"
            android:launchMode="singleTop"
            android:theme="@style/Theme.MyTheme" />
        <activity
            android:name=".mobius.ResourcesActivity"
            android:label="@string/title_activity_mobius"
            android:launchMode="singleTop"
            android:theme="@style/Theme.MyTheme">
            <intent-filter>
                <action android:name="android.intent.action.SEARCH" />
            </intent-filter>

            <meta-data
                android:name="android.app.searchable"
                android:resource="@xml/searchable" />
        </activity>
        <activity
            android:name=".mobius.ResourceShopsActivity"
            android:launchMode="standard"
            android:parentActivityName=".MITMainActivity"
            android:theme="@style/Theme.MyTheme" />
        <activity
            android:name=".mobius.ResourceTypesActivity"
            android:launchMode="standard"
            android:parentActivityName=".MITMainActivity"
            android:theme="@style/Theme.MyTheme" />
        <activity
            android:name=".mobius.ResourceSearchActivity"
            android:launchMode="standard"
            android:parentActivityName=".MITMainActivity"
            android:theme="@style/Theme.MyTheme" />
        <activity
            android:name=".mobius.ResourceListActivity"
            android:launchMode="standard"
            android:parentActivityName=".mobius.ResourcesActivity"
            android:theme="@style/Theme.MyTheme" />
        <activity
            android:name=".mobius.ResourceViewActivity"
            android:launchMode="standard"
            android:parentActivityName=".mobius.ResourceListActivity"
            android:theme="@style/Theme.MyTheme" />

        <meta-data
            android:name="com.google.android.gms.version"
            android:value="@integer/google_play_services_version" />

        <!-- Google Maps API Key -->
        <meta-data
            android:name="com.google.android.maps.v2.API_KEY"
            android:value="AIzaSyA2e9sTg9cL2oKUPbX6KQwOCYirNSJaFmQ" />

        <activity
            android:name=".shuttles.activities.ShuttleRouteActivity"
            android:launchMode="standard"
            android:parentActivityName=".MITMainActivity"
            android:theme="@style/Theme.MyTheme" />
        <activity
            android:name=".shuttles.activities.ShuttleStopActivity"
            android:launchMode="standard"
            android:parentActivityName=".MITMainActivity"
            android:theme="@style/Theme.MyTheme" />
        <activity
            android:name=".tour.activities.TourSelfGuidedActivity"
            android:label="@string/title_activity_tour_self_guided"
            android:parentActivityName=".MITMainActivity"
            android:screenOrientation="portrait"
            android:theme="@style/Theme.MyTheme" />
        <activity
            android:name=".tour.activities.TourDetailActivity"
            android:label="@string/title_activity_tour_detail"
            android:parentActivityName=".MITMainActivity"
            android:screenOrientation="portrait"
            android:theme="@style/Theme.MyTheme" />
        <activity
            android:name=".tour.activities.TourDirectionsActivity"
            android:label="@string/title_activity_tour_directions"
