        <activity
            android:name=".tour.activities.MitIntroActivity"
            android:label="@string/title_activity_intro_mit"
            android:parentActivityName=".MITMainActivity"
            android:screenOrientation="portrait"
            android:theme="@style/Theme.MyTheme" />
        <activity
            android:name=".tour.activities.TourStopActivity"
            android:parentActivityName=".MITMainActivity"
            android:screenOrientation="portrait"
            android:theme="@style/Theme.MyTheme" />
        <activity
            android:name=".emergency.activity.EmergencyContactsActivity"
            android:label="@string/title_activity_emergency_contacts"
            android:screenOrientation="portrait"
            android:theme="@style/Theme.MyTheme" />
        <activity
            android:name=".people.activity.PersonDetailActivity"
            android:label="@string/title_activity_person_detail"
            android:screenOrientation="portrait"
            android:theme="@style/Theme.MyTheme" />
        <activity
            android:name=".news.activities.NewsStoryActivity"
            android:parentActivityName=".MITMainActivity"
            android:theme="@style/Theme.MyTheme" />
        <activity
            android:name=".news.activities.NewsCategoryActivity"
            android:label="@string/title_activity_news_category"
            android:parentActivityName=".MITMainActivity"
            android:theme="@style/Theme.MyTheme" />
        <activity
            android:name=".news.activities.NewsImageGalleryActivity"
            android:label="@string/title_activity_news_image_gallery"
            android:parentActivityName=".MITMainActivity"
            android:theme="@style/Theme.MyTheme" />
        <activity
            android:name=".events.activities.CalendarsActivity"
            android:label="@string/title_activity_events_calendars"
            android:parentActivityName=".MITMainActivity"
            android:theme="@style/Theme.MyTheme" />
        <activity
            android:name=".events.activities.EventsDetailActivity"
            android:label="@string/title_activity_events_detail"
            android:theme="@style/Theme.MyTheme" />
        <activity
            android:name=".dining.activities.DiningRetailActivity"
            android:label="@string/title_activity_dining_retail"
            android:parentActivityName=".MITMainActivity"
            android:theme="@style/Theme.MyTheme" />
        <activity
            android:name=".dining.activities.FiltersActivity"
            android:label="@string/filters_activity_title"
            android:parentActivityName=".MITMainActivity"
            android:theme="@style/Theme.MyTheme" />
        <activity
