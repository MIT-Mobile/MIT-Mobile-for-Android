            android:theme="@style/Theme.MyTheme" />
        <activity
            android:name=".about.activities.AboutMitActivity"
            android:label="@string/about_about"
            android:parentActivityName=".MITMainActivity"
            android:theme="@style/Theme.MyTheme" />
        <activity
            android:name=".about.activities.CreditsActivity"
            android:label="@string/about_credits"
            android:parentActivityName=".MITMainActivity"
            android:theme="@style/Theme.MyTheme" />
        <activity
            android:name=".qrreader.activities.ScannerInfoActivity"
            android:label="@string/title_activity_scanner_info"
            android:parentActivityName=".MITMainActivity"
            android:theme="@style/Theme.MyTheme" />
        <activity
            android:name=".qrreader.activities.ScannerHistoryActivity"
            android:label="@string/title_activity_scanner_history"
            android:parentActivityName=".MITMainActivity"
            android:theme="@style/Theme.MyTheme" />
        <activity
            android:name=".qrreader.activities.ScannerHistoryDetailActivity"
